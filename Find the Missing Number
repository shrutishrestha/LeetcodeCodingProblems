Find the Missing Number

def check_missing(nums):
  for i in range(len(nums)):
    if i != nums[i]:
      return i


def find_missing_number(nums):
  nums.append(-1)
  i = 0
  while i< len(nums):
    j = nums[i]
    nums[i], nums[j] = nums[j], nums[i]
    if nums[i] == -1 or nums[i] == i:
      i += 1
  missing = check_missing(nums)
  return missing
